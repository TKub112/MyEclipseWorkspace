import java.util.Date;

public class peselData {
	private String sex;
	private Date birthDate;
	
	peselData(String sex,Date birthDate)
	{
		this.sex=sex;
		this.birthDate=birthDate;
	}
	protected void setSex(String sex)
	{
		this.sex=sex;
	}
	protected void setSex(Date birthDate)
	{
		this.birthDate=birthDate;
	}

}
